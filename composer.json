{
    "name": "auto-swagger/php-swagger-generator",
    "description": "Automatic Swagger/OpenAPI documentation generator using PHP attributes",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AutoSwagger\\": "src/"
        }
    },
    "authors": [
        {
            "name": "AutoSwagger",
            "email": "autoswagger@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/annotations": "^2.0",
        "symfony/finder": "^6.0",
        "symfony/yaml": "^7.1",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/console": "^9.0|^10.0",
        "illuminate/routing": "^9.0|^10.0",
        "nikic/php-parser": "^4.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AutoSwagger\\Laravel\\AutoSwaggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
